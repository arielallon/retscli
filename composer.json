{
  "name": "arielallon/retscli",
  "description": "A CLI for interacting with RETS servers",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ariel Allon"
    }
  ],
  "require": {
    "php": "^7.2",
    "troydavisson/phrets": "2.6.2",
    "neighborhoods/buphalo": "^1.1.0",
    "symfony/yaml": "^4.2",
    "monolog/monolog": "^2.1",
    "symfony/console": "^5.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ArielAllon\\RetsCli\\": [
        "src/",
        "fab/"
      ]
    }
  },
  "scripts": {
    "post-update-cmd": [
      "@composer dump-autoload -a"
    ],
    "post-install-cmd": [
      "@composer buphalo"
    ],
    "buphalo": [
      "bin/fab/buphalo",
      "@composer dump-autoload -a"
    ]
  }
}
